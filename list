# 多维数组操作示例
data = [[[1, 2], [3, 4]], [[5, 6], [7, 8]]]

for i in range(len(data)):
    for j in range(len(data[i])):
        for k in range(len(data[i][j])):
            data[i][j][k] *= 2

# 打印操作后的多维数组
for i in range(len(data)):
    for j in range(len(data[i])):
        print(data[i][j])
    print()
